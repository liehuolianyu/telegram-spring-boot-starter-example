<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd" >
<mapper namespace="com.github.xabgesagtx.example.dao.UserDao">
    <sql id="Base_Column_List">
        id, firstName,lastName,isBot,userName
    </sql>
    <select id="selectById" parameterType="Integer" resultType="com.github.xabgesagtx.example.entity.User">
        select *
        from User
        where id = #{id}
    </select>
    <insert id="insert" parameterType="com.github.xabgesagtx.example.entity.User" >
     insert into User(id, firstName,lastName,isBot,userName)
     values(#{id},#{firstName},#{lastName},#{isBot},#{userName})
    </insert>

</mapper>